#include<stdio.h>
int main()
{
    int val;
    int choice;
    printf("MENU\n");
    printf("Enter your choice from 1-5");
    scanf("%d",&choice);
    switch(choice)
    {
        case 1: printf("1.Pizza\n");
                printf("Price is Rs 239\n");
                break;
        case 2: printf("Food item is Pasta\n");
                printf("Price is Rs 179\n");
                break;
        case 3: printf("Food item is Sandwich\n");
                printf("Price is Rs 149\n");
                break;
        case 4: printf("Food item is French Fries\n");
                printf("Price is Rs 99\n");
                break;
        case 5: printf("Food item is Burger\n");
                printf("Price is Rs 239\n");
                break;
    }
}

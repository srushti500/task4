#include<stdio.h>
int main()
{
    int mark;
    char grade;
    printf("Enter the marks scored by student\n");
    scanf("%d",&mark);
    if(85<=mark && mark<=100)
    {
        printf("Grade A\n");
    }
    else if(70<=mark && mark<=84)
    {
        printf("Grade B\n");
    }
    else if(55<=mark && mark<=69)
    {
        printf("Grade C\n"); 
    }
     else if(40<=mark && mark<=54)
    {
        printf("Grade D\n");
    }
     else
    {
        printf("Grade F\n");
    }
}
